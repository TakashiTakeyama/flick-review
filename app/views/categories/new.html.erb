<div class="container">
  <%= form_with(model: category, local: true) do |f| %>
    <%= f.label :名前 %>
    <%= f.text_field :name %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.label :tag_list %>
    <%= f.text_field :tag_list %>

    <p>
      <%= f.submit "作成する" %>
    </p>
  <% end %>
</div>